    "nodejs",
    "backend",
    "rest-api",
    "jwt",
    "payment-gateway",
    "flutterwave",
    "email-verification",
    "otp",
    "coupon-system",
    "user-management",
    "analytics",
    "websocket",
    "real-time-chat",
    "support-tickets",
    "agent-dashboard",
    "file-upload",
    "multer"
  ],
  "author": "UYEH TECH <uyehtech@gmail.com> (https://uyehtech.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BayorTV68/uyehtechbackend.git"
  },
  "bugs": {
    "url": "https://github.com/BayorTV68/uyehtechbackend/issues",
    "email": "uyehtech@gmail.com"
  },
  "homepage": "https://uyehtech.com",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "uploads/*"
    ],
    "verbose": true,
    "execMap": {
      "js": "node"
    },
    "watch": [
      "uyeh.js",
      ".env"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json"
  },
  "config": {
    "port": 3000,
    "mongodbMemoryServer": {
      "version": "latest"
    }
  },
  "type": "commonjs"
}
